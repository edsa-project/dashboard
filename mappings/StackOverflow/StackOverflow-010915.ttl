    ns2:jobLocation "Chicago, IL" ;
    ns2:jobTitle "Senior Engineer at HERE-Chicago (Chicago, IL)" ;
    ns2:url "http://careers.stackoverflow.com/jobs/96623/senior-engineer-here-chicago" ;
