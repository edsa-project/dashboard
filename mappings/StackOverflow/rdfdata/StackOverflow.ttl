        <http://www.edsa-project.eu/skill/ecmascript-6>,
        <http://www.edsa-project.eu/skill/flux>,
        <http://www.edsa-project.eu/skill/javascript>,
        <http://www.edsa-project.eu/skill/reactjs> .

